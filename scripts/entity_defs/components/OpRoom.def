<root>

    <Properties>

        <!-- 玩家上次所在的房间 -->
        <roomKey>
            <Type> UINT64 </Type>
            <Flags> BASE_AND_CLIENT </Flags>
            <Persistent> true </Persistent>
            <Default> 1 </Default>
        </roomKey>
    </Properties>

    <BaseMethods>
    
        <enterRoom>
            <Exposed/>
            <Arg> ROOM_KEY </Arg>
        </enterRoom>

        <leaveRoom>
            <Exposed/>
        </leaveRoom>
    </BaseMethods>

    <ClientMethods>
        <enterRoomCallback>
            <Arg> UINT32 </Arg>
        </enterRoomCallback>
    </ClientMethods>
</root>